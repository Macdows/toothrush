        td.cell_center(ng-repeat="titre in dentalSheetCtrl.titre_tableau_douleur") {{titre}}
      tr(ng-repeat="key in dentalSheetCtrl.label_partie_13")
        td
          |{{key.label}}
          input.text_field_e(name='key.label', type='text', value='0', style='width:30px;text-align:center', ng-if='key.showInput')
