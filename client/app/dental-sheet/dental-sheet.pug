          input(type='checkbox', name= '{{titre}}', ng-model='checkbox_douleur_provoquee_droit')
        td.cell_center
          input(type='checkbox', name= '{{titre}}', ng-model='checkbox_douleur_identique_droit')
        td.cell_center
          select(name= '{{titre}}', ng-model='select_droite', ng-options='color for color in dentalSheetCtrl.localisationMuscle')
          input(type='text', name='{{titre}}', ng-model='autres_douleur_referees_droit')
        td.cell_center
          input(type='checkbox', name= '{{titre}}', ng-model='checkbox_douleur_provoquee_gauche')
        td.cell_center
          input(type='checkbox', name= '{{titre}}', ng-model='checkbox_douleur_identique_gauche')
        td.cell_center
          select(name= '{{titre}}', ng-model='select_droite', ng-options='color for color in dentalSheetCtrl.localisationMuscle')
          input(type='text', name='{{titre}}', ng-model='autres_douleur_referees_droit')
    br
    table.vtable
      tr
        td
          | Est-ce que l'un des mouvements effectués vous a déclenché des maux de tête ?
          label(ng-repeat="option in dentalSheetCtrl.scale")
            input(
              type='radio',
              name='13_radio_maux_tete',
              ng-checked='(option)'
            )
            span {{option}}
    table(style='width:100%')
        tr
          td.cell_center(style='width:20%;vertical-align:text-top;text-align:right')
            div Si oui lesquels ?
          td.cell_center(style='width:80%')
            textarea.text_field_e(name='13_which', rows='2', style='width:100%')
